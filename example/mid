[
    note:
    a program should have a trailing newline
]

.data
allocate _vrm 23

.prog

add     r2  r2  r3                          ; this is a comment
[
    this is a 
    multiline 
    comment
]
:mainlabel
sub     r3  r2  r2
ld      r4  r5  r7
mul     r9  r31 2013                        ; multiplies 
ld      r6  r2  0x70                        ; check for hexa 
addc    r2  r6  0b01                        ; check for binary 
